<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sbt.shopping.jaepil.AccountMapper">
<select id="getAccount" resultType="com.sbt.shopping.jaepil.AccountDTO">
	select * from account where a_id = 'mk'	
</select>

<update id="editAccount">
	update account set a_password = #{a_password} where a_id = 'mk'
</update>

<delete id="signOut">
	delete account where a_id = 'mk'
</delete>


</mapper>