<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sbt.shopping.kimoon.kakaopay.KakaoPayMapper">

<insert id="addOrder">
	insert into order_detail values
	(order_detail_seq.nextval, #{od_id}, #{od_product}, #{od_price}, #{od_cnt}, sysdate, '주문완료')
</insert>

<delete id="deleteCart">
	delete cart where c_no = #{cartNoList}
</delete>



</mapper>